    "lint": "ng lint",
    "prod": "npm run build -- --prod",
    "prettier-format": "prettier --write"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4
  },
